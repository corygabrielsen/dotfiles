{
  "name": "dotfiles",
  "version": "1.0.0",
  "description": "This is a dotfiles repository",
  "scripts": {
    "setup": "npm run setup-node",
    "setup-node": "git submodule update --init --recursive && npx ts-node install-node.ts",
    "setup-python": "git submodule update --init --recursive && python3 -m install"
  },
  "repository": "git@github.com:stoooops/dotfiles.git",
  "author": "Cory Gabrielsen",
  "license": "MIT",
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18"
  }
}
